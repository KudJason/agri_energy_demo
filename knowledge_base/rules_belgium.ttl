        "{'variable': 'min_surface', 'operator': '>=', 'value': '1 ha'}",
        "{'variable': 'parcel_type', 'operator': '=', 'value': 'arable'}",
        "{'variable': 'sigec_identified', 'operator': '=', 'value': 'yes'}" .

<http://example.org/opportunity/be/wal/all_extra_measures_md_c21_0> a core:Opportunity ;
