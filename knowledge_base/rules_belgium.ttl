@prefix core: <http://example.org/agri-energy/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://example.org/opportunity/be/wal/all_extra_measures_md_c0_0> a core:Opportunity ;
    rdfs:label "Complementary Income Support for Young Farmers"@en,
        "Aide complémentaire au revenu pour les jeunes agriculteurs"@fr ;
    core:definedBy <http://example.org/doc/be/wal/all_extra_measures_md> ;
    core:description "Annual decoupled payment per eligible hectare to support young farmers' income, with higher rates for first 50 ha, maximum 100 ha per farm or per eligible person."@en ;
    core:hasTag "Young Farmer"@en ;
    core:requiresCriterion "{'variable': 'age', 'operator': '<', 'value': '41'}",
        "{'variable': 'farmer_type', 'operator': '=', 'value': 'active'}",
        "{'variable': 'hectares', 'operator': '<=', 'value': '100'}",
        "{'variable': 'installation_years', 'operator': '<', 'value': '5'}" .

<http://example.org/opportunity/be/wal/all_extra_measures_md_c10_0> a core:Opportunity ;
    rdfs:label "372 - Cooperation in the tourism sector"@en,
        "372 – Coopération dans le domaine du tourisme"@fr ;
    core:definedBy <http://example.org/doc/be/wal/all_extra_measures_md> ;
    core:description "Financial support for transcommunal structures to develop rural tourism services focused on natural/cultural heritage and priority identities (authenticity/culture or nature/escape), through project calls."@en ;
    core:hasTag "Cooperation"@en,
        "Rural"@en,
        "Tourism"@en ;
    core:requiresCriterion "{'variable': 'scope', 'operator': '=', 'value': 'regional'}",
        "{'variable': 'structure', 'operator': '=', 'value': 'transcommunal recognised by CGT or German-speaking ministry'}" .

<http://example.org/opportunity/be/wal/all_extra_measures_md_c17_0> a core:Opportunity ;
    rdfs:label "Long Soil Cover Eco-scheme"@en,
        "éco-régime couverture longue du sol"@fr ;
    core:definedBy <http://example.org/doc/be/wal/all_extra_measures_md> ;
    core:description "Voluntary one-year commitment to cover soil from 1 January to 15 February on prairies, winter crops, intercultures, arable lands with cover regrowth, and permanent crops with inter-row vegetation. Payment based on coverage thresholds: Entry (70%+), Intermediate (80%+), Optimal (90%+), adjusted by weighting of prairies and similar covers."@en ;
    core:hasTag "Environment"@en,
        "Soil Cover"@en ;
    core:requiresCriterion "{'variable': 'coverage_percentage', 'operator': '>=', 'value': '70 + (10 * weighting_prairies)'}",
        "{'variable': 'coverage_percentage', 'operator': '>=', 'value': '80 + (10 * weighting_prairies)'}",
        "{'variable': 'coverage_percentage', 'operator': '>=', 'value': '90 + (5 * weighting_prairies)'}" .

<http://example.org/opportunity/be/wal/all_extra_measures_md_c1_0> a core:Opportunity ;
    rdfs:label "Access to the reserve of basic payment entitlements"@en,
        "Accès à la réserve de droits au paiement de base"@fr ;
    core:definedBy <http://example.org/doc/be/wal/all_extra_measures_md> ;
    core:description "Attribution de nouveaux droits au paiement de base pour jeunes ou nouveaux agriculteurs récemment installés, avec valeur égale à la moyenne régionale."@en ;
    core:hasTag "New Farmer"@en,
        "Young Farmer"@en ;
    core:requiresCriterion "{'variable': 'age', 'operator': 'young', 'value': 'farmer'}",
        "{'variable': 'installation_date', 'operator': 'within', 'value': '5 years'}",
        "{'variable': 'status', 'operator': 'is', 'value': 'new farmer'}" .

<http://example.org/opportunity/be/wal/all_extra_measures_md_c20_0> a core:Opportunity ;
    rdfs:label "Long Soil Cover Eco-scheme"@en,
        "Eco-régime couverture longue du sol"@fr ;
    core:definedBy <http://example.org/doc/be/wal/all_extra_measures_md> ;
    core:description "Voluntary annual commitment to maintain soil cover from January 1 to February 15 on a proportion of the farm's total area, with three coverage thresholds (entry, intermediate, superior) offering different payment levels."@en ;
    core:hasTag "Biodiversity"@en,
        "Climate"@en,
        "Cover Crops"@en,
        "Soil"@en ;
    core:requiresCriterion "{'variable': 'coverage_ratio', 'operator': '>=', 'value': 'threshold (70-90% + adjustment)'}",
        "{'variable': 'farmer_status', 'operator': '=', 'value': 'active'}",
        "{'variable': 'location', 'operator': 'in', 'value': 'Wallonia'}",
        "{'variable': 'sigec_identified', 'operator': '=', 'value': 'yes'}" .

<http://example.org/opportunity/be/wal/all_extra_measures_md_c20_1> a core:Opportunity ;
