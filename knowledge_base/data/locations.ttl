#################################################################

###  Countries
:Belgium rdf:type :Region ;
    rdfs:label "Belgium"@en, "Belgique"@fr, "BelgiÃ«"@nl .

:France rdf:type :Region ;
    rdfs:label "France"@en, "France"@fr .

###  Regions (Belgium)
:Wallonia rdf:type :Region ;
    rdfs:label "Wallonia"@en, "Wallonie"@fr ;
    :partOf :Belgium .

:Flanders rdf:type :Region ;
    rdfs:label "Flanders"@en, "Flandre"@fr, "Vlaanderen"@nl ;
    :partOf :Belgium .

:BrusselsCapital rdf:type :Region ;
    rdfs:label "Brussels-Capital"@en, "Bruxelles-Capitale"@fr, "Brussels Hoofdstedelijk"@nl ;
    :partOf :Belgium .

###  Object Property for Hierarchy
:partOf rdf:type owl:ObjectProperty ;
    rdfs:domain :Region ;
    rdfs:range :Region ;
    rdfs:label "part of"@en .
